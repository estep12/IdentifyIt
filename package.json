  "bugs": {
    "url": "https://github.com/hatab002/Project3/issues"
  },
  "homepage": "https://github.com/hatab002/Project3#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.0",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "mongoose": "^5.1.7",
    "react-google-login": "^3.2.1"
  }
}
